<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".old_activity.Rubishcratedataupload">

    <ScrollView
        android:id="@+id/scrolll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linerfortime"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/merchantname"
                        android:layout_width="100dp"
                        android:layout_height="40dp"
                        android:layout_gravity="left"


                        />

                    <TextView
                        android:id="@+id/timer"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center|right"
                        android:lines="2"
                        android:maxLines="2"
                        android:paddingRight="15dp"
                        android:text="Time Clock 00:00"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textSize="@dimen/clientname"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/eusername"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/name">

                <ImageView
                    android:id="@+id/missing"
                    android:layout_width="110sp"
                    android:layout_height="20sp"

                    android:gravity="center"
                    android:text="Missing Crates "
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/missing"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="@dimen/clientname" />

                <android.widget.ImageView
                    android:id="@+id/homeacti"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="right"
                    android:layout_marginRight="14dp"

                    android:src="@drawable/homeblue" />
            </LinearLayout>


            <TextView
                android:id="@+id/damagereport"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center_horizontal"
                android:text="Damage Report"

                android:textSize="18sp"
                android:textStyle="bold" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/round_corner_shape"
                android:orientation="vertical"
                android:padding="5dp"
                android:visibility="gone">


                android:weightSum="3">

                <LinearLayout
                    android:id="@+id/jobdeta"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/damager"


                    android:weightSum="3">


                    <TextView
                        android:id="@+id/jobid"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />

                    <TextView
                        android:id="@+id/projmgt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />

                    <TextView
                        android:id="@+id/date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/salesdata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/jobdeta"
                    android:layout_marginTop="10sp"

                    android:weightSum="3">


                    <TextView
                        android:id="@+id/salesrap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_weight="1"
                        android:gravity="left"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />

                    <TextView
                        android:id="@+id/tech"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />

                    <TextView
                        android:id="@+id/dated"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="TextView"
                        android:textSize="@dimen/createhometest" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/listdata"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"

                android:layout_below="@+id/salesdata">

                <ListView
                    android:id="@+id/listdatal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:visibility="gone">

                </ListView>
            </LinearLayout>


            <include
                android:id="@+id/upperheader"
                layout="@layout/customlistforuploadrubishdata_new"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/listdata"
                android:visibility="visible"

                />


            <include

                android:id="@+id/upperheader2"
                layout="@layout/customlistforuploadrubishdata_new"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/upperheader"
                android:layout_marginTop="10sp"
                android:visibility="gone"

                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_below="@+id/upperheader2"
                android:layout_marginTop="10sp"
                android:gravity="center"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:orientation="horizontal">


                <TextView

                    android:id="@+id/gobacktosca"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_gravity="left"
                    android:layout_weight="1"
                    android:background="@drawable/round_corner_btn2_new"
                    android:gravity="center"
                    android:text="Add Item"
                    android:textColor="@color/btn_text_sky_blue"
                    android:textSize="15sp" />

                <View
                    android:layout_width="10dp"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/gomissi"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:layout_weight="1"

                    android:adjustViewBounds="true"
                    android:background="@drawable/round_corner_btn2_red_new"
                    android:gravity="center"
                    android:text="Finished with Report"
                    android:textColor="@color/red_new"
                    android:textSize="15sp" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>


